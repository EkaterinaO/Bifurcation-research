Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"General", " ", "initial", " ", "conditions"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Conditions", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c1", ">", "0"}], ",", 
       RowBox[{"c2", ">", "0"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Discrete", " ", "equations"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Observed", " ", "model", " ", "is", " ", "a", " ", "discrete", " ", 
      "system"}], ",", 
     RowBox[{
     "so", " ", "we", " ", "will", " ", "treat", " ", "it", " ", "like", " ", 
      "a", " ", "proper", " ", "discrete", " ", "system"}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"discreteEq", ":=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"x", "[", 
        RowBox[{"n", "+", "1"}], "]"}], ",", 
       RowBox[{"y", "[", 
        RowBox[{"n", "+", "1"}], "]"}]}], "}"}], "\[Equal]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"y", "[", "n", "]"}], "/", "c1"}], "]"}], "-", 
        RowBox[{"y", "[", "n", "]"}]}], ",", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"x", "[", "n", "]"}], "/", "c2"}], "]"}], "-", 
        RowBox[{"x", "[", "n", "]"}]}]}], "}"}]}]}], "\n", 
   RowBox[{"generalEq", ":=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "'"}], "[", "t", "]"}], ",", 
       RowBox[{
        RowBox[{"y", "'"}], "[", "t", "]"}]}], "}"}], "\[Equal]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"y", "[", "t", "]"}], "/", "c1"}], "]"}], "-", 
        RowBox[{"y", "[", "t", "]"}]}], ",", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"x", "[", "t", "]"}], "/", "c2"}], "]"}], "-", 
        RowBox[{"x", "[", "t", "]"}]}]}], "}"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Initial discrete equation\>\"", ",", "discreteEq"}], "]"}], 
   "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Equilibrium", " ", "positions", " ", "for", " ", "discrete", " ", 
      "system", " ", "is", " ", "a", " ", "solutions", " ", "of", " ", 
      "equations", " ", 
      RowBox[{"x", "[", 
       RowBox[{"n", "+", "1"}], "]"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"x", "[", "n", "]"}], " ", "and", " ", 
       RowBox[{"y", "[", 
        RowBox[{"n", "+", "1"}], "]"}]}], "=", 
      RowBox[{"y", "[", "n", "]"}]}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Equilibrium", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"generalEq", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "\[Equal]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "[", "t", "]"}], ",", 
          RowBox[{"y", "[", "t", "]"}]}], "}"}]}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "[", "t", "]"}], ",", 
         RowBox[{"y", "[", "t", "]"}]}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Using", " ", "only", " ", "the", " ", "second", " ", "equilibrium", " ",
       "position"}], ",", 
     RowBox[{
      RowBox[{
      "because", " ", "of", " ", "the", " ", "initial", " ", "condition", " ",
        "x"}], ">", "0"}], ",", 
     RowBox[{"y", ">", "0"}]}], "*)"}], "\n", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"EquiX", "=", 
     RowBox[{
      RowBox[{"x", "[", "t", "]"}], "/.", 
      RowBox[{"Equilibrium", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}]}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"EquiY", "=", 
     RowBox[{
      RowBox[{"y", "[", "t", "]"}], "/.", 
      RowBox[{"Equilibrium", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}]}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Equilibrium position E1 = (\>\"", ",", "EquiX", ",", "EquiY", ",", 
     "\"\<)\>\""}], "]"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "We", " ", "linearize", " ", "initial", " ", "system", " ", "near", " ", 
     "the", " ", "equilibrium", " ", "point", " ", "E1", " ", "and", " ", 
     "we", " ", "obtain", " ", "the", " ", "Jacobi", " ", 
     RowBox[{"matrix", ":"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"JacobiMatrix", "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"generalEq", "[", 
             RowBox[{"[", 
              RowBox[{"2", ",", "1"}], "]"}], "]"}], ",", 
            RowBox[{"x", "[", "t", "]"}]}], "]"}], ",", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"generalEq", "[", 
             RowBox[{"[", 
              RowBox[{"2", ",", "1"}], "]"}], "]"}], ",", 
            RowBox[{"y", "[", "t", "]"}]}], "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"generalEq", "[", 
             RowBox[{"[", 
              RowBox[{"2", ",", "2"}], "]"}], "]"}], ",", 
            RowBox[{"x", "[", "t", "]"}]}], "]"}], ",", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"generalEq", "[", 
             RowBox[{"[", 
              RowBox[{"2", ",", "2"}], "]"}], "]"}], ",", 
            RowBox[{"y", "[", "t", "]"}]}], "]"}]}], "}"}]}], "}"}], "/.", 
      RowBox[{"Equilibrium", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Jacobi matrix in E1: \>\"", ",", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{"MatrixForm", "[", "JacobiMatrix", "]"}], ",", "Conditions"}], 
      "]"}]}], "]"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Eigen", " ", "values", " ", "of", " ", "Jacobi", " ", "matrix"}],
     "*)"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Eigen values of Jacobi matrix:\>\"", ",", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{"Eigenvalues", "[", "JacobiMatrix", "]"}], ",", "Conditions"}],
       "]"}]}], "]"}], "\[IndentingNewLine]", "\n", 
   RowBox[{
   "Print", "[", 
    "\"\<Eigen values are both on the Imaginary axes. The discrete system is \
\nasymptotically stable is all eigenvalues are located inside the unit circle \
\ncentred at the origin.\>\"", "]"}], "\[IndentingNewLine]", "\n", 
   RowBox[{
   "Print", "[", 
    "\"\<We are using  Jury\[CloseCurlyQuote]s stability conditions, which \
are \nanalogy of  Routh\[Dash]Hurwitz stability criterion for discrete time\>\
\"", "]"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Determinant", " ", "of", " ", "Jacobi", " ", "matrix"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"jacobian", "=", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{"Det", "[", "JacobiMatrix", "]"}], ",", "Conditions"}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{"Trace", " ", "of", " ", "Jacobi", " ", "mmatrix"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"trace", "=", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{"Tr", "[", "JacobiMatrix", "]"}], ",", "Conditions"}], "]"}]}],
     ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"so", " ", "here"}], ",", 
     RowBox[{
     "we", " ", "obtained", " ", "interval", " ", "for", " ", "asymptotic", 
      " ", "stability", " ", "of", " ", "equilibrium", " ", "point", " ", 
      "of", " ", "system"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Interval of asymptotic stability:\>\"", ",", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"Reduce", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"1", "-", "trace", "+", "jacobian"}], ">", "0"}], ",", 
          RowBox[{
           RowBox[{"1", "-", "jacobian"}], ">", "0"}], ",", 
          RowBox[{
           RowBox[{"1", "-", "trace", "+", "jacobian"}], ">", 
           RowBox[{"-", "0"}]}], ",", 
          RowBox[{"c1", ">", "0"}], ",", 
          RowBox[{"c2", ">", "0"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"c1", ",", "c2"}], "}"}]}], "]"}], "]"}]}], "]"}], 
   "\[IndentingNewLine]", "\n", 
