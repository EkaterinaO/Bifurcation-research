Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"here", " ", 
     RowBox[{"I", "'"}], "m", " ", "defining", " ", "some", " ", 
     "functions"}], ",", 
    RowBox[{
    "which", " ", "will", " ", "simplify", " ", "further", " ", 
     "calculations"}]}], "*)"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{
     "this", " ", "is", " ", "the", " ", "initial", " ", "system", " ", 
      "with", " ", "c1"}], "=", "1"}], ",", 
    RowBox[{"and", " ", "c2", " ", "is", " ", "a", " ", "parameter"}]}], 
   "*)"}], 
  RowBox[{
   RowBox[{"discF", ":=", 
    RowBox[{"Function", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "[", 
         RowBox[{"n", "+", "1"}], "]"}], ",", 
        RowBox[{"y", "[", 
         RowBox[{"n", "+", "1"}], "]"}]}], "}"}], "\[Equal]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Sqrt", "[", 
          RowBox[{"y", "[", "n", "]"}], "]"}], "-", 
         RowBox[{"y", "[", "n", "]"}]}], ",", 
        RowBox[{
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"x", "[", "n", "]"}], "/", "#"}], "]"}], "-", 
         RowBox[{"x", "[", "n", "]"}]}]}], "}"}]}], "]"}]}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"function", " ", "for", " ", "y"}], "-", 
     RowBox[{"coordinate", " ", "of", " ", "equilibrium", " ", "position"}]}],
     "*)"}], "\n", 
   RowBox[{"EqYF", ":=", 
    RowBox[{"Function", "[", 
     RowBox[{"1", "/", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", "#"}], ")"}], "^", "2"}]}], "]"}]}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"function", " ", "for", " ", "x"}], "-", 
     RowBox[{"coordinate", " ", "of", " ", "equilibrium", " ", "position"}]}],
     "*)"}], "\[IndentingNewLine]", 
   RowBox[{"EqXF", ":=", 
    RowBox[{"Function", "[", 
     RowBox[{"#", "/", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", "#"}], ")"}], "^", "2"}]}], "]"}]}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"temporary", " ", "value", " ", "for", " ", "c2"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"temp", "=", "6.2"}], ";"}], "\n", 
   RowBox[{"(*", 
    RowBox[{"There", " ", "are", " ", "two", " ", "figures", " ", 
     RowBox[{"below", ".", "Both"}], " ", "figures", " ", "show", " ", 
     "behaviour", " ", "of", " ", "the", " ", "system", " ", "with", " ", 
     "parameters", " ", "outside", " ", "of", " ", "zone", " ", "of", " ", 
     "asymptotic", " ", "stability", " ", "of", " ", "equilibrium", " ", 
     "point"}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Simply", " ", "I", " ", "took", " ", "c2", " ", "outside", " ", "of", 
     " ", "the", " ", "right", " ", "border", " ", "of", " ", "interval", " ",
      "and", " ", "took", " ", "the", " ", "initial", " ", "condition", " ", 
     "near", " ", "equilibrium", " ", "point"}], "*)"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{"I", "'"}], "m", " ", "plotting", " ", 
      RowBox[{"x", "[", "n", "]"}], " ", "and", " ", 
      RowBox[{"y", "[", "n", "]"}], " ", "separately"}], ",", 
     RowBox[{
     "because", " ", "there", " ", "is", " ", "no", " ", "point", " ", "to", 
      " ", "plot", " ", "y", 
      RowBox[{"(", "x", ")"}], " ", "according", " ", "to", " ", "the", " ", 
      "initial", " ", "meaning", " ", "of", " ", "the", " ", "model"}]}], 
    "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dataPoints", "=", 
     RowBox[{"RecurrenceTable", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"discF", "[", "temp", "]"}], ",", 
         RowBox[{
          RowBox[{"x", "[", "0", "]"}], "\[Equal]", 
          RowBox[{
           RowBox[{"EqXF", "[", "temp", "]"}], "+", "0.01"}]}], ",", 
         RowBox[{
          RowBox[{"y", "[", "0", "]"}], "\[Equal]", 
          RowBox[{
           RowBox[{"EqYF", "[", "temp", "]"}], "+", "0.01"}]}]}], "}"}], ",", 
       
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", "400"}], "}"}]}], "]"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"ListPlot", "[", 
    RowBox[{
     RowBox[{"dataPoints", "[", 
      RowBox[{"[", 
       RowBox[{";;", ",", "1"}], "]"}], "]"}], ",", 
     RowBox[{"Joined", "\[Rule]", "True"}], ",", 
     RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
     RowBox[{"AxesLabel", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"\"\<n\>\"", ",", "\"\<x[n]\>\""}], "}"}]}], ",", 
     RowBox[{
     "PlotLabel", "\[Rule]", 
      "\"\<Parameters outside of the stable region\>\""}]}], "]"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"ListPlot", "[", 
    RowBox[{
     RowBox[{"dataPoints", "[", 
      RowBox[{"[", 
       RowBox[{";;", ",", "2"}], "]"}], "]"}], ",", 
     RowBox[{"Joined", "\[Rule]", "True"}], ",", 
     RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
     RowBox[{"AxesLabel", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"\"\<n\>\"", ",", "\"\<y[n]\>\""}], "}"}]}], ",", 
     RowBox[{
     "PlotLabel", "\[Rule]", 
      "\"\<Parameters outside of the stable region\>\""}]}], "]"}], "\n", 
   "\[IndentingNewLine]"}]}]], "Input",
